<launch>
  <!-- 播放bag文件 -->
  <node name="rosbag_play" pkg="rosbag" type="play" 
        args="--clock $(find fsac_autonomous)/../vline.bag" 
        output="screen" />
  
  <!-- 启动数据处理节点 -->
  <node name="fsd_data_processor" pkg="fsac_autonomous" type="bag_processor.py" 
        output="screen" />
  
  <!-- 启动RViz可视化 -->
  <node name="rviz" pkg="rviz" type="rviz" 
        args="-d $(find fsac_autonomous)/rviz/fsd_visualization.rviz" 
        required="false" />
  
  <!-- 发布静态TF：base_link -> laser_link -->
  <node pkg="tf2_ros" type="static_transform_publisher" 
        name="base_to_laser" 
        args="2.4 0 0 0 0 0 base_link laser_link" />
        
</launch>